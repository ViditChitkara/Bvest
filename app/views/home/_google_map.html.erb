    <div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat: 28.675911, lng: 77.1131558};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 17,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map,
          draggable:true,
        });
        marker.setMap(map);
      }
    </script>

    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVqcbnlNg6NNhp7r8h_2N9XsxjycBtA2k&callback=initMap">
    </script>